@model IHS.Apps.CMP.Models.ViewModels.AandD.DocumentModel

@if (Model != null && !string.IsNullOrEmpty(Model.Content))
{
    @Html.Raw(Model.Content)
}
else
{
    @Html.Action("NoData", "Forces");
}

@if (Model != null && Model.Link != null)
{
    @Html.Partial("Widget/LinkListWidgetFooter", new[] { Model.Link })
}