@using IHS.Apps.CMP.Models.ViewModels.AandD;
@model IEnumerable<DatedItem>

@if (Model.Count() == 0)
{
    <p>There are no reports available for this market and year at this time.</p>
}
else
{
    <div class="list-group list-links ">
        @foreach (DatedItem item in Model)
        {
            <a class="list-group-item" href="@item.Link" target="_blank">
                <span class="news h4">
                    @item.Title
                    <small class="date">@item.Date.ToDisplayDate()</small>
                </span>
            </a>
        }
    </div>
}
