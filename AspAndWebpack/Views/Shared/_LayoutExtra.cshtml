@{
    Layout = "~/Views/Shared/_Layout_Template.cshtml";
}

@section jstree_js {
    <div id="jsTreeContainer"></div>

    <script type="text/javascript">
        $(function () {
            $('#jsTreeContainer').jstree({
                'core': {
                    'data': {
                        'url': '/jsTree3/GetJsTree3Data',
                        'dataType': 'json'
                    }
                }
            });
        });
    </script>

}

@RenderBody()
@Html.BuildTree()